{
  "$schema": "../test/test.schema.json",
  "resolveRefs": {
    "fn": "\\assoc\\jsonFetch",
    "in": [
      "",
      {
        "c111": 300,
        "d222": 400,
        "$ref": "utils/assoc.test/ref.json",
        "ref": {
          "$ref": "utils/assoc.test/ref1.json"
        }
      }
    ],
    "assert": "equal",
    "out": {
      "z": 200,
      "y": 2,
      "c111": 300,
      "x": 2,
      "d222": 400,
      "ref": {
          "z": 200,
          "y": 2
      }
    }
  },
  "repairIndexesRecursive": {
    "fn": "\\assoc\\repairIndexesRecursive",
    "in": [{
      "1": {
        "1": "d",
        "0": "c"
      },
      "0": {
        "1": "b",
        "0": "a"
      }
    }],
    "assert": "equal",
    "out": [["a", "b"], ["c", "d"]]
  },
  "repairIndexes": {
    "fn": "\\assoc\\repairIndexes",
    "in": [{
      "1": "a",
      "0": "b"
    }],
    "assert": "equal",
    "out": ["b", "a"]
  },
  "splitKeys": {
    "fn": "\\assoc\\splitKeys",
    "in": [{
      "a:b": {
        "c:d": "e:f",
        "x:y": "0:1"
      } 
    }],
    "assert": "equal",
    "out": {
      "a": {
        "b": {
          "c": {
            "d": "e:f"
          },
          "x": {
            "y": "0:1"
          }
        }
      }
    }
  }
}