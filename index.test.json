{
  "$schema": "../test/test.schema.json",
  "jsonFetch:resolveRefs": {
    "fn": "\\utils\\jsonFetch",
    "in": [
      "",
      {
        "c111": 300,
        "d222": 400,
        "$refs": ["utils/index.test/ref.json", "utils/index.test/ref2.json"],
        "ref": {
          "$ref": "utils/index.test/ref1.json"
        }
      }
    ],
    "assert": "equal",
    "out": {
      "z": 200,
      "y": 2,
      "c111": 300,
      "x": 2,
      "d222": 400,
      "ref": {
          "z": 200,
          "y": 2
      }
    }
  },  
  "jsonFetch:resolveRef": {
    "fn": "\\utils\\jsonFetch",
    "in": [
      "",
      {
        "c111": 300,
        "d222": 400,
        "$ref": "utils/index.test/ref.json",
        "ref": {
          "$ref": "utils/index.test/ref1.json"
        }
      }
    ],
    "assert": "equal",
    "out": {
      "z": 200,
      "y": 2,
      "c111": 300,
      "x": 2,
      "d222": 400,
      "ref": {
          "z": 200,
          "y": 2
      }
    }
  }
}